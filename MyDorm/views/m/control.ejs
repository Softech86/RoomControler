<script src="/javascripts/index_m/control.js"></script>

<div id="user-password" hidden><%= user.password %></div>


<div class="control-lock-pad">
    <div class="control-lock-bg">
        LOADING
    </div>
    <div class="control-lock-yin control-lock-yin-thick">
        <embed src="/images/lock-yin.svg" width="75" height="75"
               type="image/svg+xml"
               pluginspage="http://www.adobe.com/svg/viewer/install/" />
    </div>
    <div class="control-lock-yang control-lock-yang-thick">
        <embed src="/images/lock-yang.svg" width="75" height="75"
               type="image/svg+xml"
               pluginspage="http://www.adobe.com/svg/viewer/install/" />
    </div>
    <div class="control-lock-yin">
        <embed src="/images/lock-yin.svg" width="75" height="75"
               type="image/svg+xml"
               pluginspage="http://www.adobe.com/svg/viewer/install/" />
    </div>
    <div class="control-lock-yang">
        <embed src="/images/lock-yang.svg" width="75" height="75"
               type="image/svg+xml"
               pluginspage="http://www.adobe.com/svg/viewer/install/" />
    </div>

</div>

<div class="control-lock-btn">
    <span class="glyphicon glyphicon-lock"></span>
    <div class="control-lock-btn-remind">LOCK</div>
</div>


